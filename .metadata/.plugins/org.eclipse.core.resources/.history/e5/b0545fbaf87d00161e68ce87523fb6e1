import javax.swing.JPanel;

public class Menu extends JPanel implements ActionListener
{

	private JButton 
	
	public Menu()
	{
		
	}

	@Override
	public void actionPerformed(ActionEvent e)
	{
		this.getParent().requestFocus();
	}
	
}
